{
  "name": "codit-pdf-viewer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:server": "node server",
    "build:server": "node server",
    "start:client": "react-scripts start --prefix client",
    "build:client": "react-scripts build --prefix client",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "npm run build:server && npm run build:client"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pdfjs-dist": "^4.10.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
