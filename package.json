{
  "name": "codit-pdf-viewer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:server": "ts-node-dev --respawn server/server.ts",
    "build:server": "tsc -p server",
    "start:client": "react-scripts start --prefix client",
    "build:client": "react-scripts build --prefix client",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "npm run build:server && npm run build:client"
  },
  "dependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.5.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pdfjs-dist": "^4.10.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "concurrently": "^7.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
